#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/openai@4.56.0_zod@3.23.8/node_modules/openai/bin/node_modules:/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/openai@4.56.0_zod@3.23.8/node_modules/openai/node_modules:/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/openai@4.56.0_zod@3.23.8/node_modules:/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/openai@4.56.0_zod@3.23.8/node_modules/openai/bin/node_modules:/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/openai@4.56.0_zod@3.23.8/node_modules/openai/node_modules:/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/openai@4.56.0_zod@3.23.8/node_modules:/home/user/code/projects/Ruby-Hackathon-Project/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../openai@4.56.0_zod@3.23.8/node_modules/openai/bin/cli" "$@"
else
  exec node  "$basedir/../../../../../../openai@4.56.0_zod@3.23.8/node_modules/openai/bin/cli" "$@"
fi
